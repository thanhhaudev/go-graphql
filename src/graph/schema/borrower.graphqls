input CreateBorrowerInput {
    name: String!
    address: String!
    telNumber: String!
    birthDate: Time!
}

input UpdateBorrowerInput {
    name: String
    address: String
    telNumber: String
    birthDate: Time
}

input BorrowBookInput {
    borrowerId: ID!
    bookId: ID!
    quantity: Int!
    dueDate: Time!
}

type Borrower {
    id: ID!
    name: String!
    address: String!
    telNumber: String!
    birthDate: Time!
    books: [Book!]!
    createdAt: Time!
    updatedAt: Time!
}